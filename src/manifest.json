{
	"name": "DBDark",
	"version": "0.2.6",
	"manifest_version": 2,
	"description": "Dark theme for Databricks",
	"icons": {
		"48": "img/logo.png"
	},
	"content_scripts": [
		{
			"matches": [ "https://*.databricks.com/*", "https://*.azuredatabricks.net/*" ],
			"css": [
				"css/editor.css",
				"css/markdown.css",
				"css/cell.css",
				"css/widget.css",
				"css/body.css",
				"css/history-panel.css",
				"css/topbar.css",
				"css/context-bar.css",
				"css/sidebar.css",
				"css/filebrowser-panel.css",
				"css/data-panel.css",
				"css/search-panel.css",
				"css/user-menu.css",
				"css/help-menu.css",
				"css/shorcuts.css"
			],
			"run_at": "document_idle"
		}
	]
}
